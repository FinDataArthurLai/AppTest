target 'AppTest' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for AppTest
    pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
    pod 'GoogleMaps'
    
    target 'AppTestTests' do
        # Pods for testing
        pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
        pod 'GoogleMaps' 
    end
    
    target 'AppTestUITests' do
        # Pods for testing
        pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
        pod 'GoogleMaps' 
    end
    
end

